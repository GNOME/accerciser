#: ../plugindata/validate/basic.py:24
#, python-format
msgid "actionable %s is not focusable or selectable"
msgstr "%s yang dapat dijalankan tidak dapat difokuskan maupun dipilih"

#: ../plugindata/validate/basic.py:42
#, python-format
